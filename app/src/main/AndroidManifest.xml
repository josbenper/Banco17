<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.benavent.banco">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".PassActivity" />
        <activity android:name=".CuentasActivity" />
        <activity android:name=".MovimientosActivity" />
        <activity android:name=".TodosMovimientosActivity" />
        <activity android:name=".IngresosActivity" />
        <activity android:name=".GlobalActivity" />
        <activity android:name=".TransferenciaActivity"></activity>
    </application>

</manifest>